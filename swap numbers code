#include<stdio.h>
#include<conio.h>
int main()
{
	int first, second, temp;
	printf("Enter first number:");
	scanf("%d", &first);
	printf("Enter second number:");
	scanf("%d", &second);
	
	
	temp = first;
	
	first = second;
	
	second = temp;
	
	printf("\nAfter swapping, first number is:%d\n", first);
	printf("After swapping, second number is:%d\n", second);
	return 0;
}
