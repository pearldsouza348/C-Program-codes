#include<stdio.h>
#include<conio.h>
int main()
{
	int mark;
	printf("Enter a number:");
	scanf("%d",&mark);
	if(mark>85&&mark<=100)
	{
		printf("A Grade");
	}
	else if(mark>70&&mark<=84)
	{
		printf("B grade");
	}
	else if(mark>55&&mark<69)
	{
		printf("C Grade");
	}
	else if(mark>40&&mark<54)
	{
		printf("D Grade");
	}
	else if(mark>=0&&mark<40)
	{
		printf("F Grade");
	}
	return 0;
}
